{
  "name": "biganfa/php-stratum",
  "description": "A stored procedure and function loader, wrapper generator for MySQL in PHP.",
  "keywords": [
    "MySQL",
    "Stored Routines",
    "Stored Procedures",
    "Stored Functions",
    "Wrapper",
    "Loader",
    "PHP"
  ],
  "type": "tool",
  "license": "MIT",
  "authors": [
    {
      "name": "Set Based IT Consultancy",
      "email": "info@setbased.nl",
      "homepage": "http://www.setbased.nl"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^4.0 || ^5.0",
    "setbased/phing-extensions": "^2.0.1"
  },
  "require": {
    "php": ">=5.4.0",
    "setbased/exception": "^1.0.0",
    "setbased/helper-code-store-mysql": "^1.0.0",
    "setbased/helper-code-store-php": "^1.0.1",
    "setbased/php-affirm": "^0.1.2",
    "symfony/console": "^2.8 || ^3.0",
    "zendframework/zend-code": ">=2.0"
  },
  "bin": [
    "bin/stratum"
  ],
  "autoload": {
    "psr-4": {
      "SetBased\\Stratum\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\Stratum\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin"
  }
}
