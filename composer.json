{
  "name": "biganfa/php-stratum",
  "description": "A stored procedure and function loader, wrapper generator for MySQL in PHP.",
  "keywords": [
    "MySQL",
    "Stored Routines",
    "Stored Procedures",
    "Stored Functions",
    "Wrapper",
    "Loader",
    "PHP"
  ],
  "type": "tool",
  "license": "MIT",
  "require-dev": {
    "phpunit/phpunit": "^6.0.0",
    "setbased/phing-extensions": "^2.0.1"
  },
  "require": {
    "php": ">=7.0.0",
    "setbased/exception": "^1.0.0",
    "setbased/helper-code-store-mysql": "^1.1.0",
    "setbased/helper-code-store-php": "^1.1.0",
    "setbased/php-affirm": "^0.2.0",
    "symfony/console": "^3.0.0",
    "zendframework/zend-code": "^3.0.0"
  },
  "bin": [
    "bin/stratum"
  ],
  "autoload": {
    "psr-4": {
      "SetBased\\Stratum\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\Stratum\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/"
  }
}
